body{
    background-color:rgb(237,237,239);
	margin: 0;
	padding: 0;
	border: 0;
}
.com-no-display {
    display: none;
}
.com-display {
    display: block;
}
button,img {
    cursor:pointer;
}
#navigation {
    background-color: rgb(36,41,44);
    height:80px;
}
#navigation #logo {
    margin-left:120px;
    margin-top:4px;
}
#navigation button {
	font-family: "微软雅黑";
	font-size: 18px;
	background-color: rgb(36,41,44);
    position: relative;
    bottom: 20px;
	margin-left: 55px;
	padding-bottom: 14px;
	padding-top: 14px;
	border: 0;
}
#navigation button.display {
	color: white;
}
#navigation button.no-display {
	color: gray;
}
#navigation button:hover {
	color: white;
}
#navigation input {
    border-style: none;
    width: 220px;
    height: 27px;
    background-color:black;

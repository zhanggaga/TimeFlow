article {
    float: left;
    position: relative;
    margin-right:6px;
    margin-bottom:6px;
}
    
#content article:nth-child(4n+4) {  
    margin-right: 0;  
}  
.post-format-content {  
    position: relative;  
    background: #111;  
    /*z-index: 1;*/
}  
.post-thumbnail {  
    max-width: 100%;  
    height: auto;  
    overflow: hidden;  
    /*position: relative;
    z-index: 4;*/
}  
.content-wrap {  
    padding: 0;  
    position: absolute;  
    text-align: center;  
    width: 100%;  
    top: 0;  
    bottom: 0;  
    display: table-cell;  
    vertical-align: middle;  
    overflow: hidden;  
}  

.client_name:hover {
	cursor: pointer;
}
.featured-image {  
    display: table-cell;  
    position: relative;  
    transition: opacity .25s ease-in-out, background .25s ease-in-out;  
    -moz-transition: opacity .25s ease-in-out, background .25s ease-in-out;  
    -webkit-transition: opacity .25s ease-in-out, background .25s ease-in-out;  
    vertical-align: middle;  
    z-index: 1;  
    color: #fff;  
    text-decoration: none;  
    opacity: 0;  
    width:260px;
    height:260px; 
}  
.featured-image:hover {  
    opacity: 0.9;  
    color: #fff;  
    background: rgba(0,0,0,0.8);  
    cursor:pointer;
}  
.post-thumbnail img {  
    display: block;  
} 

.photoContainer {
	width: 260px;
	height: 260px;
	display: inline-block;
	margin-right: 6.6px;
	margin-bottom: 6px;
} 
.photoContainer article, .photoContainer article .post-format-content,
.photoContainer article .post-format-content .post-thumbnail {
	height: 100%;
	width: 100%;
} 

    background-repeat:no-repeat;
    color:white;
}

#navigation #user {
    background-image: url(../img/user.png); 
    width: 28px;
    height: 30px;
    margin-left: 130px;
    position: relative;
    bottom: 25px;
}

div#container {
    width: 800px;
    margin-left: auto;
    margin-right: auto;
    font-family:SimHei;
    color: #444;
}
div#container #userArea {
    margin-top: 60px;
    border: 1px solid rgba(198,198,198,0.8);
    height: 190px; 
    background-color: rgb(250, 250,250);
    /*background-image: url("../img/bc3.png");*/
    background-repeat:no-repeat;
    background-size: 80px;
    background-position-y: 20px;
    background-position-x: 20px;
    box-shadow: 1px 2px 1px #aaa;
}
div#container #myPhoto {
    margin-left: 20px;
    margin-top: 20px;
    margin-bottom: 20px;
    min-width: 100px;
    /*height: 50*/
}
div#container #userintro {
    display: block;
    margin-left: 20px;
    margin-top: 20px;
    margin-bottom: 35px;
}
#userintro #textDetail {
	float: right;
	position: relative;
	right: 550px;
}
#followBtn {
	margin-left: 15px;
	border: 1px solid rgba(198,198,198,0.8);
	background-color: #fff;
	padding-top: 6px;
	padding-bottom: 6px;
	padding-left: 18px;
	padding-right: 18px;	
	color: #204D74;
}
#userintro #textDetail:hover {
	cursor: default;
}
#userintro #imgDetail img{
	width: 80px;
}
div#container #userintro > span {
    font-family:SimHei;
}
div#container #userintro > span:first-child {
    font-size: 20px;
    color: #000;
}
div#container #userintro > span:last-child {
    color: gray;
    position: relative;
    top: 20px;
}
div#container #userInfor {
    height: 40px;
}
div#container #userInfor div{
    float: left;
    border: 1px solid #999;
    text-align: center;
    padding-left: 82px;
    padding-right: 82px;
    padding-bottom: 8px;
    padding-top: 8px;
    border-bottom: 0;
    font-family:SimHei;
    color: gray;
}
div#container #userInfor #photo {
        color: #49a2ed;
    }
div#container #userInfor div:hover {
    cursor: pointer;
    color: #49a2ed;
}
div#container #userInfor div:first-child {
    border-left: 0;
    padding-left: 85px;
}
div#container #userInfor div:last-child {
    border-right: 0;
    padding-right:87px;
}

  #more{
    width: 800px;
    margin:0 auto;
    margin-top:40px;
    background-color:rgb(237,237,239);
    border:none;
}
    #more_logo {
    width: 122px;
    height: 116px;
    border: none;
    background-image: url(../img/more.png);
    background-color:rgb(237,237,239);
    margin-left:42%;
}
    article {
    float: left;
    position: relative;
    margin-right:6px;
    margin-bottom:6px;
}
    
#content article:nth-child(4n+4) {  
    margin-right: 0;  
}  
.post-format-content {  
    position: relative;  
    background: #111;  
}  
.post-thumbnail {  
    max-width: 100%;  
    height: auto;  
    overflow: hidden;  
}  
.content-wrap {  
    padding: 0;  
    position: absolute;  
    text-align: center;  
    width: 100%;  
    top: 0;  
    bottom: 0;  
    display: table-cell;  
    vertical-align: middle;  
    overflow: hidden;  
}  


.featured-image {  
    display: table-cell;  
    position: relative;  
    transition: opacity .25s ease-in-out, background .25s ease-in-out;  
    -moz-transition: opacity .25s ease-in-out, background .25s ease-in-out;  
    -webkit-transition: opacity .25s ease-in-out, background .25s ease-in-out;  
    vertical-align: middle;  
    z-index: 1;  
    color: #fff;  
    text-decoration: none;  
    opacity: 0;  
    width:260px;
    height:260px; 
}  
.featured-image:hover {  
    opacity: 0.9;  
    color: #fff;  
    background: rgba(0,0,0,0.8);  
    cursor:pointer;
}  
.post-thumbnail img {  
    display: block;  
} 
#PhotoContainer {
	margin-top: 40px;
} 
.photoContainer {
	width: 260px;
	height: 260px;
	display: inline-block;
	margin-right: 6.6px;

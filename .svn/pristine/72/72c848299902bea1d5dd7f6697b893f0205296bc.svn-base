body{
    background-color:rgb(237,237,239);
	margin: 0;
	padding: 0;
	border: 0;
}
.com-no-display {
    display: none;
}
.com-display {
    display: block;
}
button {
    cursor:pointer;
}
#navigation {
    background-color: rgb(36,41,44);
    height:80px;
}
#navigation #logo {
    margin-left:120px;
    margin-top:4px;
}
#navigation button {
	font-family: "微软雅黑";
	font-size: 18px;
	background-color: rgb(36,41,44);
    position: relative;
    bottom: 20px;
	margin-left: 55px;
	padding-bottom: 14px;
	padding-top: 14px;
	border: 0;
}
#navigation button.display {
	color: white;
}
#navigation button.no-display {
	color: gray;
}
#navigation button:hover {
	color: white;
}
#navigation input {
    border-style: none;
    width: 220px;
    height: 27px;
    background-color:black;
    border-radius: 30px;
    margin-left: 150px;
    position: relative;
    bottom: 22px;
    padding-left:35px;
    background-image:url(../img/fangda.png);
    background-position-x:10px;
    background-position-y:8px;
    background-repeat:no-repeat;
    color:white;
}

#navigation #user {
    background-image: url(../img/user.png); 
    width: 28px;
    height: 30px;
    margin-left: 130px;
    position: relative;
    bottom: 25px;
}
#addphoto #add{
    margin-left: 48%;
    margin-top: 25px;
    margin-bottom: 25px;
    width: 85px;
    height: 85px;
	background-image: url(../img/add.png); 
    background-color:rgb(237,237,239);
    border:none;
}
#content {
    width: 800px;
    margin:0 auto;
    background-color: rgb(237,237,239);
    padding: 0;
    border: 0;
}
#content #num1,#num2,#comments{
    width: 800px;
    background-color: white;

}
#content  #user1,#user2{
    width: 60px;
    height: 60px;
	background-image: url(../img/user1.png); 
    border:none;
    margin-left: 15px;
    margin-top:15px;
}
#user2 {
    background-image: url(../img/user2.png);
}
#content span.nikename {
    font-family: "微软雅黑";
	font-size: 15px;
    color:gray;
    margin-left:17px;
    position: relative;
    bottom: 22px;
}
#content  #photo1,#photo2 {
    width: 600px;
    margin-left: 100px;
}
#content  .label {
    margin-left: 100px;
    margin-top:25px;
   
}
.Iabel {
    margin-left: 100px;
    margin-bottom: 17px;
}
#content span.label {
    font-family: "微软雅黑";
	font-size: 10px;
    color:rgb(198,198,198);
    margin-left:10px;
    position: relative;
    top:-2px;
}
#content #num2 span.label {
    position: relative;
    top: -19px;
}

#content  .comment {
    width: 20px;
    height: 18px;
	background-image: url(../img/comment.png); 
    background-color:white;
    border:none;
    margin-left: 560px;
    position: relative;
    bottom: 20px;
}
#content #num2 .comment {
     bottom: 35px;
}
#content  .share {
    width: 16px;
    height: 16px;
    background-image: url(../img/share.png);
    background-color:white;
    border: none;
    margin-left: 20px;
    position: relative;
    bottom: 22px;
}
#content #num2 .share {
    bottom: 37px;
}
#content  .like {
     width: 19px;
     height: 16px;
     background-image: url(../img/like2.png);
     background-color:white;
     border: none;
     margin-left: 20px;
     position: relative;
     bottom: 22px;
}
#content #num2 .like {
     background-image: url(../img/like1.png);
     bottom: 37px;
}

     #content #num1 #la{
        width: 11px;
        height: 8px;
        background-image: url(../img/la.png);
        background-color:white;
        border: none;
        margin-left:100px;
    }

#comments{
    border-top:1px solid rgba(198,198,198,0.6);

}
#comments #com {
    margin-left: 100px;
    margin-top: 25px;
    width:450px;
    height:30px;

}
#comments #publish {
    margin-left: 50px;
    margin-top: 25px;
    width:90px;
    height:38px;
    border:0;
    background-color: #8db800;
	color: white;
    font-family: "微软雅黑";
	font-size: 15px;
    letter-spacing: 10px;
    line-height: 10px;
    padding-left:15px;
}
#comments   button.luser {
    width: 30px;
    height: 30px;
    margin-left: 100px;
    background-color: white;
    border: none;
}
#c1 button.luser {
    background-image: url(../img/c1.png);
   
}
#c2 button.luser {
    background-image: url(../img/c2.png);
}
#c3 button.luser {
    background-image: url(../img/c3.png);
}


#comments  span {
    font-family: "微软雅黑";
	font-size: 8px;
}
#comments span.lnn {
     position: relative;
     bottom: 21px;
    color: rgb(109,147,194);
}
#comments span.com{
    margin-left:30px;
    color: black;
    position: relative;
    bottom: 21px;
}
#c2 span.com {
    margin-left: 23px;
}
#comments span.time{
    margin-left:155px;
    color: black;
    position: relative;
    bottom: 19px;
}
#comments #reply {
    margin-left:28px;
    position: relative;
    bottom: 19px;
    border:0;
    font-family: "微软雅黑";
	font-size: 8px;
    color: rgb(109,147,194);
    background-color:white;
    height:17px;
}
#comments #allcom {
	font-size: 8px;
    color:rgb(198,198,198);
    margin-left: 95px;
    letter-spacing: 0.5px;
    border:0;
    background-color:white;
}
#comments  #shou {
     width: 11px;
     height: 8px;
     background-image: url(../img/shou.png);
     background-color:white;
     border: none;
     margin-left: 100px;
     margin-top:15px;
}
#c3 {
